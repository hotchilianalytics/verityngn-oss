# This file shows the structure of secrets needed for Streamlit Community Cloud
# DO NOT commit actual secrets! Add them via Streamlit Cloud Dashboard

# VerityNgn API Configuration
VERITYNGN_API_URL = "https://your-ngrok-url.ngrok-free.app"

# Alternative structure (also supported)
[api]
url = "https://your-ngrok-url.ngrok-free.app"
base_url = "https://your-ngrok-url.ngrok-free.app"

# Optional: Google Cloud credentials (if running workflows directly in UI)
# For API-only mode, these are NOT needed
# GOOGLE_APPLICATION_CREDENTIALS = "/path/to/service-account.json"
# GOOGLE_CLOUD_PROJECT = "your-project-id"

